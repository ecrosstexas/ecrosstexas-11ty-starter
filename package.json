{
  "name": "ecrosstexas-11ty-starter",
  "version": "1.0.0",
  "description": "A starter repo for projects using Eleventy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ecrosstexas/ecrosstexas-11ty-starter.git"
  },
