  <meta property="og:title" content="{{ pageTitle }}" />
  <meta property="og:description" content="{{ pageDescription }}" />
  <meta property="og:image" content="{{ pageSocialImg }}" />
  <meta property="og:url" content="{{ meta.siteURL }}{{ page.url | url }}" />
  <meta property="og:site_name" content="{{ meta.siteName }}" />
  <meta property="og:type" content="{{ pageType }}" />
{% if PageType == "article" %}
  <meta property="article:published_time" content="{{ page.date.toISOString() }}" />
{% endif %}

  <meta name="twitter:card" content="summary_large_image" />
  <meta name="twitter:image" content="{{ pageSocialImg }}" />
  <meta name="twitter:image:alt" content="image description" />
  <meta name="twitter:description" content="{{ pageDescription }}" />
  <meta name="twitter:title" content="{{ pageTitle }}" />
{% if meta.twitterUsername %}
  <meta name="twitter:creator" content="@{{ meta.authorTwitter }}" />
{% endif %}
  <meta name="twitter:site" content="{{ meta.siteTwitter }}" />

{% if noindex == true %}
  <meta name="robots" content="noindex,follow">
{% endif %}
  <meta name="msapplication-TileColor" content="{{ favicon.msTileColor }}">
  <meta name="theme-color" content="{{ favicon.androidThemeColor }}">

  <link rel="canonical" href="{{ pageCanonical }}"/>
  <link rel="apple-touch-icon" sizes="180x180" href="{{ favicon.appleTouchIcon }}">
  <link rel="icon" type="image/png" sizes="32x32" href="{{ favicon.favicon32 }}">
  <link rel="icon" type="image/png" sizes="16x16" href="{{ favicon.favicon16 }}">
  <link rel="manifest" href="/site.webmanifest">
  <link rel="mask-icon" href="{{ favicon.appleSafariMask }}" color="{{ favicon.appleSafariMaskColor }}">
{% if meta.indieauth == "true" %}
  <link rel="authorization_endpoint" href="{{ indieauthAuth }}">
  <link rel="token_endpoint" href="{{ indieauthToken }}">
{% endif %}
{% if meta.humansTXT == "true" %}
  <link rel="author" href="/humans.txt" />
 {% endif %} 
  <link rel="alternate" type="application/atom+xml" title="{{ meta.siteName }} Atom Feed-" href="/atom.xml" />
  <link rel="alternate" title="{{ meta.siteName }} JSON Feed" type="application/feed+json" href="/feed.json" />
  
  <link rel="stylesheet" type="text/css" href="/css/style.css"/>
{% if prism == true %}
  <link rel="stylesheet" type="text/css" href="/css/prism-tomorrow.css">
{% endif %}
<script src="https://kit.fontawesome.com/b142cce581.js" crossorigin="anonymous"></script>